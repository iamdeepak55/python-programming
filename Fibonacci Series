
# program to find  fibonacci series also check the  no. is +ve or -ve

def fib(n):
    a=0
    b=1
    if n==0:
        print("no values")
    elif n==1:
      print(a)
    else:
        print(a)
        print(b)
        for i in range(2,n):
            c=a+b
            a=b
            b=c
            print(c)


n=int(input("how many values you want to print "))

if n<0:
    print(" enter value is negative  ")
else:
    fib(n)
