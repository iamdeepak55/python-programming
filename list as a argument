# program to count even or odd numbers in a list
def count_even_odd(list):
    e=0n 
    o=0
    for i in list:
      if i%2==0:
          e+=1
      else:
          o+=1
    return e,o
list=[5,41,42,3,43,34,53,63,34,34,74,84,940,34,14]

even,odd=count_even_odd(list)


print("Even = {} , Odd = {}".format(even,odd))
