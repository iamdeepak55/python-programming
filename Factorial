# program to find factorial of a given number
def fact(n):
    f=1
    i=1
    if n==0:
        return 1
    elif n==1:
        return 1
    else:
      while(i<=n):
        f=f*i
        i+=1
    return f
#  function  to find factorial using recursion
def factorial(n):
    if n==0:
        return 1
    elif n==1:
        return 1
    else:
        return n * factorial(n-1)


n=int(input("enter the number "))
result=fact(n)
result1=factorial(n)
print(result)
print(result1)
